{
  "hosting": {
    "target": "app",
    "public": "frontend/app/dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/manifest.json",
        "destination": "/manifest.json"
      },
      {
        "source": "/icon.svg",
        "destination": "/icon.svg"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
